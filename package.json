{
  "name": "name-later",
  "version": "1.0.0",
  "description": "something...don't know what but something!",
  "main": "src/client/client.jsx",
  "scripts": {
    "test": "mocha --compilers js:babel-register src/test/**/*.js*",
    "start": "npm run build && npm run server",
    "dev": "npm run build && nodemon --exec babel-node src/server/server.js",
    "server": "babel-node src/server/server.js",
    "mkdir": "mkdir -p build",
    "build": "npm run clean && npm run mkdir && npm run build:html && npm run build:js",
    "watch": "npm run watch:html & npm run watch:js",
    "clean": "rm -rf build",
    "build:html": "npm run clean:html && cp src/public/index.html build/",
    "watch:html": "npm run build:html && chokidar src/public/index.html -c 'npm run build:html'",
    "clean:html": "rm -f build/index.html",
    "build:js": "npm run clean:js && webpack",
    "watch:js": "npm run build:js && webpack --watch",
    "clean:js": "rm -f build/$npm_package_name.$npm_package_version.js build/$npm_package_name.$npm_package_version.js.map"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rockchalkwushock/name-later.git"
  },
  "keywords": [
    "express"
  ],
  "author": "Cody Brunner <rockchalkwushock@icloud.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rockchalkwushock/name-later/issues"
  },
  "homepage": "https://github.com/rockchalkwushock/name-later#readme",
  "dependencies": {
    "bcrypt": "^0.8.7",
    "body-parser": "^1.15.2",
    "chai": "^3.5.0",
    "enzyme": "^2.6.0",
    "express": "^4.14.0",
    "mongoose": "^4.7.1",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "validator": "^6.2.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.8",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "eslint": "^3.11.1",
    "eslint-config-rallycoding": "^3.1.0",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "react-addons-test-utils": "^15.4.1",
    "webpack": "^1.13.3"
  }
}
